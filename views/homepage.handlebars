
<section class="dashboard-margin">
 
     <a href="/dashboard"><button>Create New Post</button></a></a>
   
</section>

<ol class="post-list">
  {{#each posts as |post|}}
  <li>
    <article class="post">
      <div class="title">
        <a href="/post/{{post.id}}">{{post.title}}</a>
       
      </div>
      <div class="meta">
    by {{post.user.username}} on {{format_date created_at}}
        |
        <a href="/post/{{post.id}}">{{post.comments.length}} comment(s)</a>
      </div>
    </article>
  </li>
  {{/each}}
</ol>



